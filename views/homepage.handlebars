{{! this is the homepage the user can navigate to }}
<main>
  {{#if blogPost}}
    <div class="card">
      {{#each blogPost}}
        <h3>{{title}}</h3>
        <p>Posted by {{author}} on {{formatDate blogPost}}</p>
      {{/each}}
    </div>
  {{else}}
    <p>No posts available.</p>
  {{/if}}
</main>